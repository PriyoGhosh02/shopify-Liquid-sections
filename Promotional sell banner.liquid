  "name": "Holiday Sale Banner",
  "tag": "section",
  "class": "section-holiday-sale",
  "settings": [
    {
      "type": "color",
      "id": "background_color",
      "label": "Background Color",
      "default": "#8B1A1A"
    },
    {
